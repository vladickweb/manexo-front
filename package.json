{
  "name": "manexo.com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "preview": "vite preview",
    "prepare": "husky",
    "generate:hooks": "node scripts/generateHooks.cjs && eslint --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "find-deadcode": "ts-prune | grep -v '(used in module)'",
    "knip": "knip"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "@mantine/core": "^8.0.0",
    "@mantine/hooks": "^8.0.0",
    "@radix-ui/react-slider": "^1.3.2",
    "@react-google-maps/api": "^2.20.6",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-query": "^5.68.0",
    "axios": "^1.8.3",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.6.3",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.503.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.4.1",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.14",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
